{
  "hover.floatMaxWidth": 90,
  "hover.floatMaxHeight": 20,
  "signature.maxWindowHeight": 15,
  "list.maxPreviewHeight": 25,
  "rust-analyzer.diagnostics.disabled": [
    "incorrect-ident-case",
  ],
  "list.normalMappings": {
    "h": "action:split",
  },
  "rust-analyzer.server.extraEnv": {
    "RA_LOG": "info,salsa::derived::slot=warn,flycheck=trace,rust_analyzer::main_loop=warn"
  },
  "rust-analyzer.updates.channel": "nightly",
  "coc.preferences.formatOnSaveFiletypes": [
    "rust"
  ],
  "languageserver": {
    "nix": {
      "command": "rnix-lsp",
      "filetypes": [
        "nix"
      ]
    },
    "haskell": {
      "command": "hls-nix",
      "args": ["--lsp"],
      "rootPatterns": ["*.cabal", "stack.yaml", "cabal.project", "package.yaml", "hie.yaml"],
      "filetypes": ["haskell", "lhaskell"],
      "settings": {
        "languageServerHaskell": {
          "formattingProvider": "stylish-haskell"
        }
      }
    }
  },
  "prettier.tabWidth": 4,
  "codeLens.enable": true,
  "python.pythonPath": "/home/jade/bin/_pyright_python",
  "coc.preferences.rootPatterns": [
    ".python-root",
    ".git",
    ".hg",
    ".projections.json",
  ],
  "rust-analyzer.server.path": "/home/jade/dev/rust-analyzer/target/release/rust-analyzer",
}
